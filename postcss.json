{
  "use": ["autoprefixer", "postcss-reporter", "postcss-pxtorem"],
  "local-plugins": true,
  "autoprefixer": {
    "browsers": ["> 2%"]
  },
  "postcss-pxtorem": {
    "propWhiteList": [
      "font",
      "font-size",
      "line-height",
      "letter-spacing",
      "padding",
      "padding-top",
      "padding-right",
      "padding-bottom",
      "padding-left",
      "margin",
      "margin-top",
      "margin-right",
      "margin-bottom",
      "margin-left",
      "width",
      "height",
      "border-radius",
      "letter-spacing"
    ],
    "rootValue": 16,
    "unitPrecision": 5,
    "selectorBlackList": [],
    "replace": false,
    "mediaQuery": true,
    "minPixelValue": 0
  }
}
